<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<Nullable>enable</Nullable>
		<Platforms>AnyCPU</Platforms>
		<OutputType>Library</OutputType>
		<PackageTags>notio</PackageTags>
		<PackageIcon>Notio.png</PackageIcon>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>

    <Version>0.3.81</Version>
    <PackageVersion>0.3.81</PackageVersion>
    
		<ReleaseNotes>Initial release of Notio.Network.Web package.</ReleaseNotes>
		<Description>Notio.Network.Web is a .NET library for managing "WebSocket, WebApi,..." functionality.</Description>
	</PropertyGroup>

	<Import Project="..\Notio.Build.props" />

  <PropertyGroup Condition="'$(Configuration)' == 'Release'">
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

	<ItemGroup>
    <ProjectReference Include="..\Notio.Shared\Notio.Shared.csproj" />
		<ProjectReference Include="..\Notio.Serialization\Notio.Serialization.csproj" />
	</ItemGroup>
  
</Project>
