<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<Nullable>enable</Nullable>
		<Platforms>AnyCPU</Platforms>
		<OutputType>Library</OutputType>
		<PackageTags>notio</PackageTags>
		<PackageIcon>Notio.png</PackageIcon>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>

    <Version>0.3.62</Version>
    <PackageVersion>0.3.62</PackageVersion>
    
		<ReleaseNotes>Initial release of Notio.Serialization package.</ReleaseNotes>
		<Description>Notio.Serialization is a .NET library for managing serialization json functionality.</Description>
	</PropertyGroup>

	<Import Project="..\Notio.Build.props" />

  <PropertyGroup Condition="'$(Configuration)' == 'Release'">
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

	<ItemGroup>
		<ProjectReference Include="..\Notio.Common\Notio.Common.csproj" />
	</ItemGroup>

</Project>